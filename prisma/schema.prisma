generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int     @id @default(autoincrement())
  firstname String
  lastname  String
  sex       String
  age       Int
  createdAt DateTime @default(now())

  @@map("users") // Map to the users table
}
