{
   "compilerOptions": {
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "noEmit": false,
      "esModuleInterop": true,
      "module": "commonjs",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "plugins": [
         {
            "name": "next"
         }
      ],
      "paths": {
         "@/*": ["./*"]
      },
      "outDir": "./dist",
      "moduleResolution": "node"
   },
   "include": [
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx",
      "pages/**/*",
      ".next/types/**/*.ts"
   ],
   "exclude": ["node_modules"]
}
